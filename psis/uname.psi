extern int uname(struct utsname *u);
function psi\uname(array &$u = NULL) : int {
	let u = calloc(1, struct utsname);
	return to_int(uname);
	set $u = to_array(*u,
		to_string(sysname),
		to_string(nodename),
		to_string(release),
		to_string(version), 
		to_string(machine),
		to_string(domainname));
}